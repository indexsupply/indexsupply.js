{
  "name": "sqlapi.js",
  "version": "0.0.1",
  "description": "A Typescript wrapper for Index Supply Co's SQL API",
  "main": "dist/server-index.js",
  "browser": "dist/browser-index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": ["dist"],
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build:server": "bun build src/index.ts --outfile=dist/server-index.js --target=node",
    "build:browser": "bun build src/index.ts --outfile=dist/browser-index.js --target=browser",
    "build:types": "bunx tsc --emitDeclarationOnly --declaration src/index.ts --outDir dist --skipLibCheck",
    "build": "bun run build:server && bun run build:browser && bun run build:types"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "cross-fetch": "^4.0.0",
    "viem": "^2.21.40"
  }
}
